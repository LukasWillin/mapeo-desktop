{
  "name": "mapfilter-desktop",
  "description": "electron app that wraps mapfilter",
  "author": "Stephen Whitmore <sww@eight45.net>",
  "version": "0.0.8",
  "repository": {
    "type": "git",
    "url": "git://github.com/noffle/mapfilter-desktop.git"
  },
  "homepage": "https://github.com/noffle/mapfilter-desktop",
  "bugs": "https://github.com/noffle/mapfilter-desktop/issues",
  "main": "app.js",
  "scripts": {
    "lint": "standard",
    "start": "mkdirp build && browserify src/index.js -o build/index.js && electron app.js --disable-http-cache",
    "rebuild-leveldb": "node bin/rebuild_leveldb.js",
    "rebuild-rabin": "node bin/rebuild_rabin.js",
    "rebuild-sqlite3": "node bin/rebuild_sqlite3.js",
    "rebuild-electron-deps": "npm run rebuild-leveldb && npm run rebuild-rabin && npm run rebuild-sqlite3",
    "create-windows-installer": "node bin/package.js windows && node bin/create_windows_installer.js",
    "create-macos-installer": "node bin/package.js macos && node bin/create_macos_installer.js",
    "postinstall": "npmpd"
  },
  "keywords": [],
  "pre-commit": [
    "lint"
  ],
  "dependencies": {
    "JSONStream": "^1.3.0",
    "babel-polyfill": "^6.20.0",
    "body": "^5.1.0",
    "ddem-observation-server": "github:mojodna/ddem-observation-server#integration",
    "ecstatic": "^2.1.0",
    "electron-config": "^0.2.1",
    "electron-debug": "^1.1.0",
    "hyperlog-sneakernet-replicator": "^1.1.4",
    "minimist": "^1.2.0",
    "osm-p2p": "^1.4.0",
    "osm-p2p-observations": "^2.0.0",
    "pump": "^1.0.1",
    "randombytes": "^2.0.3",
    "react-dom": "^15.4.1",
    "react-mapfilter": "github:mojodna/mapfilter#integration",
    "split2": "^2.1.0",
    "sqlite3": "^3.1.8",
    "through2": "^2.0.3",
    "websocket-stream": "^3.3.3",
    "xhr": "^2.3.1"
  },
  "darwinDependencies": {
    "appdmg": "0.4.5"
  },
  "devDependencies": {
    "babelify": "^7.3.0",
    "brfs": "^1.4.3",
    "browserify": "^13.1.1",
    "devtron": "^1.4.0",
    "dotenv": "^2.0.0",
    "electron": "1.4.10",
    "electron-packager": "^8.3.0",
    "electron-winstaller": "^2.4.1",
    "mkdirp": "^0.5.1",
    "npm-execspawn": "^1.3.0",
    "npm-platform-dependencies": "0.0.12",
    "pre-commit": "~1.1.3",
    "rimraf": "^2.5.4",
    "standard": "~8.3.0",
    "urlencode": "^1.1.0"
  },
  "license": "ISC",
  "browserify": {
    "transform": [
      "brfs"
    ]
  }
}
